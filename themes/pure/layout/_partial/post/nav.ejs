<nav class="bar bar-footer clearfix" data-stick-bottom>
  <div class="bar-inner">
  <% if (post.prev || post.next){ %>
  <ul class="pager pull-left">
    <% if (post.prev){ %>
    <li class="prev">
      <a href="<%- url_for(post.prev.path) %>" title="<%= post.prev.title || '(no title)' %>"><i class="icon icon-angle-left" aria-hidden="true"></i><span>&nbsp;&nbsp;<%= __('nav.newer') %></span></a>
    </li>
    <% } %>
    <% if (post.next){ %>
    <li class="next">
      <a href="<%- url_for(post.next.path) %>" title="<%= post.next.title || '(no title)' %>"><span><%= __('nav.older') %>&nbsp;&nbsp;</span><i class="icon icon-angle-right" aria-hidden="true"></i></a>
    </li>
    <% } %>
   
  </ul>
  <% } %>

 
</nav>